{
  "name": "svelte-highlight",
  "version": "7.0.0",
  "license": "MIT",
  "description": "Syntax Highlighting for Svelte using highlight.js",
  "author": "Eric Liu (https://github.com/metonym)",
  "sideEffects": [
    "src/styles/*.css"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build:lib": "node scripts",
    "package": "yarn build:lib && svelte-package",
    "check": "svelte-check --workspace tests",
    "test": "vitest",
    "format": "prettier --ignore-path .gitignore --write ."
  },
  "dependencies": {
    "highlight.js": "11.7.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "1.0.0",
    "@sveltejs/kit": "1.0.1",
    "@sveltejs/package": "^1.0.1",
    "@testing-library/svelte": "^3.2.2",
    "@testing-library/user-event": "^14.4.3",
    "@types/prettier": "^2.7.2",
    "carbon-components-svelte": "^0.70.13",
    "carbon-icons-svelte": "^11.4.0",
    "carbon-preprocess-svelte": "^0.9.1",
    "jsdom": "^20.0.3",
    "prettier": "^2.8.1",
    "prettier-plugin-svelte": "^2.9.0",
    "svelte": "^3.55.0",
    "svelte-check": "^3.0.1",
    "svelte-focus-key": "^0.3.2",
    "svelte-preprocess": "^5.0.0",
    "svelte2tsx": "^0.6.0",
    "totalist": "^3.0.0",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "vitest": "^0.26.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/metonym/svelte-highlight.git"
  },
  "homepage": "https://github.com/metonym/svelte-highlight",
  "bugs": "https://github.com/metonym/svelte-highlight/issues",
  "keywords": [
    "svelte",
    "svelte 3",
    "svelte components",
    "highlight.js",
    "highlight",
    "code",
    "syntax",
    "typescript"
  ]
}
